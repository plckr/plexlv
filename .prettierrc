{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte", "@ianvs/prettier-plugin-sort-imports"],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],

  "importOrder": [
    "<BUILTIN_MODULES>",
    "^svelte([/].+)?$",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[$]",
    "^[.]"
  ]
}
