{
  "name": "plexlv",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "npm-run-all --parallel vite typesafe-i18n",
    "build": "vite build",
    "preview": "vite preview",
    "vite": "vite dev",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "svelte-kit sync && storybook build",
    "typesafe-i18n": "typesafe-i18n"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@storybook/addon-essentials": "^7.0.0-beta.50",
    "@storybook/addon-interactions": "^7.0.0-beta.50",
    "@storybook/addon-links": "^7.0.0-beta.50",
    "@storybook/addon-svelte-csf": "^3.0.0-next.3",
    "@storybook/blocks": "^7.0.0-beta.50",
    "@storybook/svelte": "^7.0.0-beta.50",
    "@storybook/sveltekit": "^7.0.0-beta.50",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.30.4",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte3": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-load-config": "^4.0.1",
    "postcss-mixins": "^9.0.4",
    "postcss-preset-env": "^7.8.3",
    "prettier": "^2.8.7",
    "prettier-plugin-svelte": "^2.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-rc.8",
    "svelte": "^3.57.0",
    "svelte-check": "^3.1.4",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typesafe-i18n": "^5.24.3",
    "typescript": "^5.0.2",
    "vite": "^4.5.2",
    "vitest": "^0.29.8"
  },
  "type": "module",
  "dependencies": {
    "accept-language": "^3.0.18",
    "xml-js": "^1.6.11",
    "zod": "^3.22.3"
